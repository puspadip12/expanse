penses] = useState([])\n    const [error, setError] = useState(null)\n\n    //calculate incomes\n    const addIncome = async (income) => {\n        const response = await axios.post(`${BASE_URL}add-income`, income)\n            .catch((err) =>{\n                setError(err.response.data.message)\n            })\n        getIncomes()\n    }\n\n    const getIncomes = async () => {\n        const response = await axios.get(`${BASE_URL}get-incomes`)\n        setIncomes(response.data)\n        console.log(response.data)\n    }\n\n    const deleteIncome = async (id) => {\n        const res  = await axios.delete(`${BASE_URL}delete-income/${id}`)\n        getIncomes()\n    }\n\n    const totalIncome = () => {\n        let totalIncome = 0;\n        incomes.forEach((income) =>{\n            totalIncome = totalIncome + income.amount\n        })\n\n        return totalIncome;\n    }\n\n\n    //calculate incomes\n    const addExpense = async (income) => {\n        const response = await axios.post(`${BASE_URL}add-expense`, income)\n            .catch((err) =>{\n                setError(err.response.data.message)\n            })\n        getExpenses()\n    }\n\n    const getExpenses = async () => {\n        const response = await axios.get(`${BASE_URL}get-expenses`)\n        setExpenses(response.data)\n        console.log(response.data)\n    }\n\n    const deleteExpense = async (id) => {\n        const res  = await axios.delete(`${BASE_URL}delete-expense/${id}`)\n        getExpenses()\n    }\n\n    const totalExpenses = () => {\n        let totalIncome = 0;\n        expenses.forEach((income) =>{\n            totalIncome = totalIncome + income.amount\n        })\n\n        return totalIncome;\n    }\n\n\n    const totalBalance = () => {\n        return totalIncome() - totalExpenses()\n    }\n\n    const transactionHistory = () => {\n        const history = [...incomes, ...expenses]\n        history.sort((a, b) => {\n            return new Date(b.createdAt) - new Date(a.createdAt)\n        })\n\n        return history.slice(0, 3)\n    }\n\n\n    return (\n        <GlobalContext.Provider value={{\n            addIncome,\n            getIncomes,\n            incomes,\n            deleteIncome,\n            expenses,\n            totalIncome,\n            addExpense,\n            getExpenses,\n            deleteExpense,\n            totalExpenses,\n            totalBalance,\n            transactionHistory,\n            error,\n            setError\n        }}>\n            {children}\n        </GlobalContext.Provider>\n    )\n}\n\nexport const useGlobalContext = () =>{\n    return useContext(GlobalContext)\n}",[],"/Users/pusp/Mixture/Design Work/expence Tracker/expense-tracker_fullstack-master/frontend/src/styles/Layouts.js",[],[],"/Users/pusp/Mixture/Design Work/expence Tracker/expense-tracker_fullstack-master/frontend/src/Components/Orb/Orb.js",[],[],"/Users/pusp/Mixture/Design Work/expence Tracker/expense-tracker_fullstack-master/frontend/src/Components/Dashboard/Dashboard.js",["108"],[],"/Users/pusp/Mixture/Design Work/expence Tracker/expense-tracker_fullstack-master/frontend/src/Components/Navigation/Navigation.js",["109"],[],"/Users/pusp/Mixture/Design Work/expence Tracker/expense-tracker_fullstack-master/frontend/src/Components/Income/Income.js",["110","111"],[],"/Users/pusp/Mixture/Design Work/expence Tracker/expense-tracker_fullstack-master/frontend/src/Components/Expenses/Expenses.js",["112","113","114"],[],"/Users/pusp/Mixture/Design Work/expence Tracker/expense-tracker_fullstack-master/frontend/src/Components/Expenses/ExpenseForm.js",[],[],"/Users/pusp/Mixture/Design Work/expence Tracker/expense-tracker_fullstack-master/frontend/src/History/History.js",[],[],"/Users/pusp/Mixture/Design Work/expence Tracker/expense-tracker_fullstack-master/frontend/src/utils/Icons.js",[],[],"/Users/pusp/Mixture/Design Work/expence Tracker/expense-tracker_fullstack-master/frontend/src/utils/menuItems.js",[],[],"/Users/pusp/Mixture/Design Work/expence 